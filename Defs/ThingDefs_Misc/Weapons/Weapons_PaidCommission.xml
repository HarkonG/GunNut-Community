<Defs>

  <!--===== SR-25 =====-->
  <!--===== A semi automatic sniper =====-->
  <ThingDef ParentName="BaseBullet">
    <defName>GN_Bullet_SRTwentyFive</defName>
    <label>SR-25 bullet</label>
    <graphicData>
      <texPath>Things/Projectile/Bullet_Small</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <projectile>
      <damageDef>Bullet</damageDef>
      <damageAmountBase>18</damageAmountBase>
      <stoppingPower>1.5</stoppingPower>
      <speed>100</speed>
    </projectile>
  </ThingDef>

  <ThingDef ParentName="GN_BaseHumanMakeableGun">
    <defName>GN_SRTwentyFive</defName>
    <label>SR-25</label>
    <description>The SR-25 is a designated marksman rifle designed by Eugene Stoner. This variant, the M110, is chambered in 7.62x51 NATO and includes a small bipod, 20 round mag, a long suppresor and a precision scope.</description>
    <graphicData>
      <texPath>Things/Weapons/GN_SRTwentyFive</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>1.1</drawSize>
    </graphicData>
    <equippedAngleOffset>45</equippedAngleOffset>
    <comps>
      <li Class="CompOversizedWeapon.CompProperties_OversizedWeapon">
        <eastOffset>(0.15,0,0)</eastOffset>
        <westOffset>(-0.15,0,0)</westOffset>
      </li>
    </comps>
    <soundInteract>Interact_Rifle</soundInteract>
    <recipeMaker>
      <recipeUsers>
        <li>TableMachining</li>
      </recipeUsers>
      <researchPrerequisite>GN_Research</researchPrerequisite>
      <skillRequirements>
        <Crafting>7</Crafting>
      </skillRequirements>
    </recipeMaker>
    <statBases>
      <WorkToMake>45000</WorkToMake>
      <Mass>4</Mass>
      <AccuracyTouch>0.45</AccuracyTouch>
      <AccuracyShort>0.68</AccuracyShort>
      <AccuracyMedium>0.82</AccuracyMedium>
      <AccuracyLong>0.86</AccuracyLong>
      <RangedWeapon_Cooldown>1.5</RangedWeapon_Cooldown>
    </statBases>
    <costList>
      <Steel>65</Steel>
      <Chemfuel>10</Chemfuel>
      <ComponentIndustrial>8</ComponentIndustrial>
    </costList>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>GN_Bullet_SRTwentyFive</defaultProjectile>
        <warmupTime>2.8</warmupTime>
        <range>44</range>
        <soundCast>GN_Shot_SRTwentyFive</soundCast>
        <soundCastTail>GunTail_Medium</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
    <tools>
      <li>
        <label>stock</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>9</power>
        <cooldownTime>2</cooldownTime>
      </li>
    </tools>
  </ThingDef>

